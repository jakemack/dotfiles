# Local/private config goes in the include
[include]
  path = ~/.gitconfig.local
[alias]
  wtf     = !$DOTFILES_PATH/bin/git-wtf
  count   = !git shortlog -sn
[color]
  ui = auto
[core]
  autocrlf = input
  excludesfile = ~/.gitignore
  editor = nano
  pager =
[merge]
  ff = only
[mergetool]
  keepBackup = false
[difftool]
  prompt = false
[push]
  default = simple
[pager]
  log = less
[tag]
  sort = v:refname
